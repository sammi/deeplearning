   "source": [
    "# NDArray"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from mxnet import nd\n",
    "x = nd.ones((3,4))\n",
    "y = nd.ones((3,4))\n",
    "\n",
    "before = id(y)\n",
    "y[:] = y + x\n",
    "id(y) == before"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "z = nd.zeros_like(x)\n",
    "before = id(z)\n",
    "nd.elemwise_add(x, y, out=z)\n",
    "id(z) == before"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "before = id(x)\n",
    "x += y\n",
    "id(x) == before"
   ]
  }
 ],
